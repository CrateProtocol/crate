{
  "name": "@crateprotocol/crate-sdk",
  "version": "0.6.0",
  "description": "Fractional ownership of shared assets",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "repository": "https://github.com/CrateProtocol/crate.git",
  "author": "Jen Zhang <jen@crate.so>",
  "bugs": {
    "url": "https://github.com/CrateProtocol/crate/issues",
    "email": "team@crate.so"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://crate.so",
  "license": "AGPL-3.0",
  "devDependencies": {
    "@project-serum/anchor": "^0.24.2",
    "@rushstack/eslint-patch": "^1.1.3",
    "@saberhq/anchor-contrib": "^1.12.65",
    "@saberhq/chai-solana": "^1.12.65",
    "@saberhq/eslint-config": "^1.12.65",
    "@saberhq/solana-contrib": "^1.12.65",
    "@saberhq/token-utils": "^1.12.65",
    "@saberhq/tsconfig": "^1.12.65",
    "@solana/web3.js": "^1.39.1",
    "@types/bn.js": "^5.1.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "^16.11.27",
    "@types/prettier": "^2.6.0",
    "@yarnpkg/doctor": "^4.0.0-rc.2",
    "bn.js": "^5.2.0",
    "chai": "^4.3.4",
    "eslint": "^8.14.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^7.0.4",
    "jsbi": "^4.3.0",
    "lerna": "^4.0.0",
    "lint-staged": "^12.4.0",
    "mocha": "^9.2.2",
    "prettier": "^2.6.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.3"
  },
  "scripts": {
    "build": "rm -fr dist/ && tsc -P tsconfig.build.json && tsc -P tsconfig.esm.json",
    "docs:generate": "typedoc --excludePrivate --includeVersion --out site/ts/ src/index.ts",
    "typecheck": "tsc",
    "idl:generate": "./scripts/parse-idls.sh && ./scripts/generate-idl-types.sh",
    "idl:generate:nolint": "./scripts/parse-idls.sh && RUN_ESLINT=none ./scripts/generate-idl-types.sh",
    "lint": "eslint . --cache",
    "test:e2e": "anchor test --skip-build tests/*.ts",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "@project-serum/anchor": ">=0.24.2",
    "@saberhq/anchor-contrib": "^1.12",
    "@saberhq/solana-contrib": "^1.12",
    "@saberhq/token-utils": "^1.12",
    "@solana/web3.js": "^1.37"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "superstruct": "^0.15.4",
    "tiny-invariant": "^1.2.0",
    "tslib": "^2.4.0"
  },
  "resolutions": {
    "chai": "=4.3.4"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{md,json,js,yml,yaml}": "prettier --write"
  },
  "files": [
    "dist/",
    "src/"
  ]
}
